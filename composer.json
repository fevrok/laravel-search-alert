{
    "name": "fevrok/laravel-search-alert",
    "description": "Save search queries and notify users when new records added!",
    "license": "MIT License",
    "authors": [{
        "name": "Abdellah Chadidi",
        "email": "chadidi97@gmail.com",
        "homepage": "https://www.chadidi.dev"
    }],
    "homepage": "https://github.com/fevrok/laravel-search-alert",
    "keywords": ["Laravel", "package", "search-alert"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5|~6",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Fevrok\\SearchAlert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fevrok\\SearchAlert\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fevrok\\SearchAlert\\SearchAlertServiceProvider"
            ],
            "aliases": {
                "SearchAlert": "Fevrok\\SearchAlert\\Facades\\SearchAlert"
            }
        }
    }
}
